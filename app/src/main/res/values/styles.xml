<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@android:color/holo_green_dark</item>
        <item name="colorPrimary">@color/primary_dark_material_light_2</item>
    </style>

    <style name="material" parent="Theme.AppCompat.NoActionBar" >
        <item name="colorPrimary">@android:color/background_dark</item>
    </style>

    <style name="materiald" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="colorPrimaryDark">@color/primary_dark_material_light_1</item>
        <item name="colorPrimary">@color/colorPrimaryDark</item>
    </style>

    <style name="ssadsd" parent="Theme.AppCompat.Light.NoActionBar" >
        <item name="colorPrimaryDark">@color/primary_dark_material_light_2</item>
    </style>

</resources>
